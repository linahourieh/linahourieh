#Given two strings s and t of equal length.
#the Hamming distance between s and t, denoted dH(s,t), is the number of corresponding symbols that differ in s and t.

def first_function():
    global s
    global t
    s = input("Enter your first sequence: ")
    t = input("enter your second sequence: ")
    s = list(s)
    t = list(t)
    if len(s) != len(t):
        print("You have 2 strings of different length")
        first_function()


first_function()

print("Your first sequence is: ", s)
print("Your second sequence is: ", t)
dh = 0

for i in range(len(s)):
    if s[i] != t[i]:
        dh += 1

print("Your Hamming distance is : ", dh)
